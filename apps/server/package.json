    "@error-ingestor/shared": "workspace:*",
    "@hono/zod-validator": "^0.4.1",
    "hono": "^4.6.0",
    "neverthrow": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
