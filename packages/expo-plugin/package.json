{
  "name": "@error-ingestor/expo-plugin",
  "version": "0.1.0",
  "description": "Expo config plugin for automatic source map uploads to Error Ingestor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "app.plugin.js"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external @expo/config-plugins",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "expo",
    "expo-plugin",
    "error-tracking",
    "source-maps",
    "react-native"
  ],
  "dependencies": {
    "@expo/config-plugins": "^8.0.0"
  },
  "peerDependencies": {
    "expo": ">=51.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.0"
  }
}
